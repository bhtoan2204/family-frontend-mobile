{
  "name": "mobile_shell",
  "displayName": "mobile_shell",
  "expo": {
    "name": "mobile_shell",
    "slug": "mobile_shell",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "backgroundModes": ["fetch"] 
    },
    "platforms": [
      "ios",
      "android"
    ],
    "backgroundFetch": {
      "enabled": true,
      "minimumInterval": 15 
    },
    "notification": {
      "icon": "src/assets/images/notification-icon.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true
    },
    "android": {
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ]
    }
   
}
}
